import java.util.*;
public class Pro
{
    public static void main (String[] args)
    {
            Scanner sc = new Scanner(System.in);
            int product=1;
            System.out.println("Enter the number: ");
            int n = sc.nextInt();
            while(n>0){
                int digit = n%10;
                product *= digit;
                n/=10;
            }
            System.out.println("Product of digits of the number "+product+" .");
        
    }
}
